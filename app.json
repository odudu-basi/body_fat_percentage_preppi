{
  "expo": {
    "name": "BodyMaxx",
    "slug": "Body_percentage_preppi",
    "version": "2.7.0",
    "orientation": "portrait",
    "icon": "./assets/splash-icon.png",
    "userInterfaceStyle": "dark",
    "scheme": "preppi",
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#121212"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "-5.Preppi-AI",
      "buildNumber": "15",
      "usesAppleSignIn": true,
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "preppi"
            ]
          }
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/splash-icon.png",
        "backgroundColor": "#E85D04"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.preppi.ai"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "556dfebe-70d0-4560-a4ab-665cee803289"
      }
    },
    "owner": "odudu_basi",
    "plugins": [
      "expo-font",
      "expo-apple-authentication",
      "expo-dev-client",
      [
        "expo-build-properties",
        {
          "ios": {
            "deploymentTarget": "15.1"
          },
          "android": {
            "minSdkVersion": 21
          }
        }
      ]
    ]
  }
}
