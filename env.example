# BodyMaxx Environment Variables
# Copy this file to .env and fill in your actual keys
# Do NOT commit .env to version control

# OpenAI API Key - Meal analysis, nutrition plan generation, recipe suggestions
OPENAI_API_KEY=your_openai_api_key_here

# Supabase - Database, authentication, storage
SUPABASE_URL=your_supabase_url_here
SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Mixpanel - Analytics and event tracking
MIXPANEL_TOKEN=your_mixpanel_token_here

# Resend - Email service
RESEND_API_KEY=your_resend_api_key_here

# Together AI - AI Image Generation
TOGETHER_AI_API_KEY=your_together_ai_api_key_here

# RevenueCat - In-App Purchases & Subscriptions
# Get from RevenueCat Dashboard → Projects → [Your Project] → API Keys
# For iOS: Use the key starting with "appl_"
REVENUECAT_API_KEY=your_revenuecat_api_key_here

# Superwall - Paywall UI (get from Superwall Dashboard → Settings → Keys)
SUPERWALL_API_KEY=your_superwall_public_api_key_here

